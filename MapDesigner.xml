<?xml version="1.0" encoding="utf-8" ?>
<screenpart BackgroundColor="Transparent" HorizonScrollbar="Hidden" Margin="0px" Padding="0px" Serializer="DBFX.Serializer.FormPartSerializer">

  <xns>
    <ns k="wc"  n="DBFX.Web.Controls" />
    <ns k="lc"  n="DBFX.Web.LayoutControls" />
    <ns k="dv"  n="DBFX.Design" />
  </xns>

  <controls>
    <c T="wc:Panel"  Name="SVGPanelDesignerPanel" Shadow="none" BorderWidth="0px" Margin="2px 0px 2px 0px" MinWidth="230px" BackgroundColor="#ddd">
           <controls>

            <c T="wc:BreakLine" BackgroundColor="lightgray"  BorderWidth="0px"    Height="1px" Margin="2px" />

            <c T="wc:Label" Text="地图类型:" Margin="4px 2px 2px 16px" Float="left"/>
            <c T="wc:ComboBox"  Display="block"  Margin="2px 14px 2px 16px" Height="26px"  DataBindings="{&quot;Path&quot;:&quot;MapStyle&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}" >
             <Items>
                  <Item Text="普通地图" Value="default"  ImageUrl="" />
                  <Item Text="简易国家地图" Value="DisCountry"  ImageUrl="" />
                  <Item Text="简易省图" Value="DisProvince"  ImageUrl="" />
                  <Item Text="简易世界地图" Value="DisWorld"  ImageUrl="" />
             </Items>
            </c>
            <c T="wc:BreakLine" BackgroundColor="lightgray"  BorderWidth="0px"    Height="1px" Margin="2px" />
            <c T="wc:Label" Text="地图样式:" Margin="4px 2px 2px 16px" Float="left"/>
            <c T="wc:ComboBox"  Display="block"  Margin="2px 14px 2px 16px" Height="26px"  DataBindings="{&quot;Path&quot;:&quot;AMapStyle&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}" >
             <Items>
                  <Item Text="标准" Value="normal"  ImageUrl="" />
                  <Item Text="马卡龙" Value="macaron"  ImageUrl="" />
                  <Item Text="涂鸦" Value="graffiti"  ImageUrl="" />
                  <Item Text="远山黛" Value="whitesmoke"  ImageUrl="" />
                  <Item Text="幻影黑" Value="dark"  ImageUrl="" />
                  <Item Text="草色青" Value="fresh"  ImageUrl="" />
                  <Item Text="极夜蓝" Value="darkblue"  ImageUrl="" />
                  <Item Text="靛青蓝" Value="blue"  ImageUrl="" />
                  <Item Text="月光银" Value="light"  ImageUrl="" />
                  <Item Text="雅士灰" Value="grey"  ImageUrl="" />
                  <Item Text="酱籽" Value="wine"  ImageUrl="" />
             </Items>
            </c>
             <c T="wc:BreakLine" BackgroundColor="lightgray"  BorderWidth="0px"    Height="1px" Margin="2px" />
             <c T="wc:Label" Text="位置刷新时间间隔(ms):" Margin="4px 2px 2px 16px" Float="left"/>
             <c T="wc:TextBox" Display="block" Height="26px" Margin="2px 14px 2px 16px" DataBindings="{&quot;Path&quot;:&quot;LiveLocationTime&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}" />

           <c T="wc:BreakLine" BackgroundColor="lightgray"  BorderWidth="0px"    Height="1px" Margin="2px" />
           <c T="wc:Label" Text="缩放级别:" Margin="4px 2px 2px 16px" Float="left"/>
           <c T="wc:TextBox" Display="block" Height="26px" Margin="2px 14px 2px 16px" DataBindings="{&quot;Path&quot;:&quot;Zoom&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}" />

            <c T="wc:BreakLine" BackgroundColor="lightgray" Height="1px" Margin="4px 2px 4px 2px"/>
            <c T="wc:CheckedBox" Text="工具栏 " Margin="3px 2px 2px 16px" DataBindings="{&quot;Path&quot;:&quot;ShowTools&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}"/>
            <c T="wc:CheckedBox" Text="区域面 " Margin="3px 2px 2px 4px" DataBindings="{&quot;Path&quot;:&quot;ShowBG&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}"/>

            <c T="wc:BreakLine" BackgroundColor="lightgray" Height="1px" Margin="4px 2px 4px 2px"/>
            <c T="wc:CheckedBox" Text="道路 " Margin="3px 2px 2px 16px" DataBindings="{&quot;Path&quot;:&quot;ShowRoad&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}"/>
            <c T="wc:CheckedBox" Text="建筑物 " Margin="3px 2px 2px 4px" DataBindings="{&quot;Path&quot;:&quot;ShowBuilding&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}"/>

            <c T="wc:BreakLine" BackgroundColor="lightgray" Height="1px" Margin="4px 2px 4px 2px"/>
            <c T="wc:CheckedBox" Text="卫星图层 " Margin="3px 2px 2px 16px" DataBindings="{&quot;Path&quot;:&quot;ShowSatellite&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}"/>
            <c T="wc:CheckedBox" Text="路网图层 " Margin="3px 2px 2px 4px" DataBindings="{&quot;Path&quot;:&quot;ShowRoadNet&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}"/>

            <c T="wc:BreakLine" BackgroundColor="lightgray" Height="1px" Margin="4px 2px 4px 2px"/>
            <c T="wc:CheckedBox" Text="工具条 " Margin="3px 2px 2px 16px" DataBindings="{&quot;Path&quot;:&quot;ShowToolBar&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}"/>
            <c T="wc:CheckedBox" Text="比例尺 " Margin="3px 2px 2px 4px" DataBindings="{&quot;Path&quot;:&quot;ShowScale&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}"/>

            <c T="wc:BreakLine" BackgroundColor="lightgray" Height="1px" Margin="4px 2px 4px 2px"/>
            <c T="wc:CheckedBox" Text="鹰眼 " Margin="3px 2px 2px 16px" DataBindings="{&quot;Path&quot;:&quot;ShowOverView&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}"/>
            <c T="wc:CheckedBox" Text="地图切换 " Margin="3px 2px 2px 4px" DataBindings="{&quot;Path&quot;:&quot;ShowMapType&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}"/>

            <c T="wc:BreakLine" BackgroundColor="lightgray" Height="1px" Margin="4px 2px 4px 2px"/>
            <c T="wc:CheckedBox" Text="标注 " Margin="3px 2px 2px 16px" DataBindings="{&quot;Path&quot;:&quot;ShowPoint&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}"/>
            <c T="wc:CheckedBox" Text="拖拽平移 " Margin="3px 2px 2px 4px" DataBindings="{&quot;Path&quot;:&quot;DragEnable&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}"/>

            <c T="wc:BreakLine" BackgroundColor="lightgray" Height="1px" Margin="4px 2px 4px 2px"/>
            <c T="wc:CheckedBox" Text="双击放大 " Margin="3px 2px 2px 16px" DataBindings="{&quot;Path&quot;:&quot;DoubleClickZoom&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}"/>
            <c T="wc:CheckedBox" Text="支持缩放 " Margin="3px 2px 2px 4px" DataBindings="{&quot;Path&quot;:&quot;ZoomEnable&quot;,&quot;Mode&quot;:&quot;OneWay&quot;,&quot;Source&quot;:&quot;&quot;}"/>

           </controls>
       </c>

    <c T="dv:TypeEditors.EventList" Name="EventListBox" Text="事件处理程序" Shadow="node" Margin="2px 0px 2px 0px" MinWidth="64px">
    </c>
  </controls>
</screenpart>